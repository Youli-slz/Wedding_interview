.Time{
    width: 90rpx;
}
.day{
    font-size: 46rpx;
    color:#1d1d1d;
    vertical-align:bottom;
}
.month{
    font-size: 24rpx;
    color: #1d1d1d;
}
.clock{
    font-size: 26rpx;
    color: #5c5c5c;
    height: 70rpx;
    line-height: 70rpx;
}

.trends_content{
    width: 80%;
}

.content_word{
    font-size: 30rpx;
    color: #1d1d1d;
    margin-top: 6rpx;
}

 .content_pic{
    width: 100%;
    display: flex;
    flex-wrap: wrap;
} 

