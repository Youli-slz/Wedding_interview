.sendorsave{
    height: 90rpx;
    border-top: 10rpx solid #fff9fb;
    border-bottom: 10rpx solid #fff9fb; 
    background-color: #fff;
    display: flex;
    flex-direction: column;
    justify-content: center;
    padding-left:20rpx;
    padding-right: 20rpx;
}

#banner{
    width: 100%;
    height: 160rpx;
}

.banner{
    height: 160rpx;
     border-bottom: 10rpx solid #fff9fb; 
}

.ss_box{
    display: flex;
    justify-content: space-around;
}

#send, #save{
    width: 192rpx;
    height: 43rpx;
}

.today{
    display: flex;
    padding-left: 30rpx;
    padding-top: 30rpx;
    background-color: #fff;
}

.today .time{
    font-size: 36rpx;
    color: #1d1d1d;
    width: 90rpx;
}

#add_image{
    width: 130rpx;
    height: 130rpx;
}

.trends_list{
